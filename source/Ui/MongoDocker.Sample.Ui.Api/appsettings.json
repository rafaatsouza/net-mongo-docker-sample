{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongoDb": {
    "Server": "mongodb://mongo",
    "Database": "local",
    "Collection": "default"
  },
  "LogSeq": {
    "ServerUrl": "http://logseq",
    "ApiKey": "${LOG_TOKEN}",
    "MinimumLevel": "Trace",
    "LevelOverride": {
      "Microsoft": "Warning"
    }
  }
}
