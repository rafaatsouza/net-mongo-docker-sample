{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RepositoryConfiguration": {
    "MongoServer": "mongodb://mongo",
    "MongoDatabase": "local",
    "MongoCollection": "default"
  },
  "LogSeq": {
    "ServerUrl": "http://logseq"
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ]
  }
}